
choice
	prompt "Architecture select"
	default ARCH_ARM64

config ARCH_ARM64
	bool "ARM64"

config ARCH_AARCH64
	bool "AARCH64"

config ARCH_RISCV64
	bool "RISCV64"

config ARCH_RISCV32
	bool "RISCV32"
endchoice


